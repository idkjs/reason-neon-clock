{
  "name": "neon-clock-parcel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel start serve",
    "serve": "yarn parcel index.html",
    "start": "parcel src/index.html",
    "prestart": "npm run clean:bs && npm run build:bs",
    "watch:bs": "bsb -make-world -w",
    "build": "parcel build src/index.html",
    "prebuild": "npm run clean && npm run build:bs",
    "build:bs": "bsb -make-world",
    "clean": "npm run clean:bs && npm run clean:dist",
    "clean:bs": "bsb -clean-world",
    "clean:dist": "rm -rf dist",
    "test": "npm run build && jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "bs-platform": "5.0.6",
    "bsb-js": "^1.1.7",
    "less": "3.10.3",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "^1.12.3"
  },
  "dependencies": {
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "reason-react": "0.7.0"
  }
}
